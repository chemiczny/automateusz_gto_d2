    For(i' := 0, i' < 2 * l1 + 1, i'++) [
        For(j' := 0, j' < 2 * l2 + 1, j'++) [
            For(k' := 0, k' < 2 * l3 + 1, k'++) [
                For(l' := 0, l' < 2 * l4 + 1, l'++) [
                    newResult := 0;
                    For(i := 0, i < (l1 + 1) * (l1 + 2) / 2, i++) [
                        If ( Not IsZero(N(A1[i'+1][i+1])),
                            For(j := 0, j < (l2 + 1) * (l2 + 2) / 2, j++) [
                                If ( Not IsZero(N(A2[j'+1][j+1] )) ,
                                    For(k := 0, k < (l3 + 1) * (l3 + 2) / 2, k++) [
                                        If ( Not IsZero(N(A3[k'+1][k+1] )) ,
                                            For(l := 0, l < (l4 + 1) * (l4 + 2) / 2, l++) [


    A1 := CartesianToSphericalMatrix[l1 + 1];
    A2 := CartesianToSphericalMatrix[l2 + 1];
    A3 := CartesianToSphericalMatrix[l3 + 1];
    A4 := CartesianToSphericalMatrix[l4 + 1];


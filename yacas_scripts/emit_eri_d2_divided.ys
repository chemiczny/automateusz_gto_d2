                        derivE := Concat( derivE, extract[2] );
                        d2r2 := Concat( d2r2, extract[5] );
                        dr2 := Concat( dr2, extract[4] );
                        r2 := Concat( r2, extract[3] );
                        
                        //znajduje wyrazenia proste, ktore juz sie pojawily
                        h := CSE'SubstituteTaggedSubexpressions(csl, h);
              
                        {h, t} := CSE(h);

                        csl := Concat(csl, t);

                        g[i][j][k][l][caseIt] := h;
                        caseIt := caseIt + 1;
                        
                    ];
